import React from 'react';
import './PieceStats.css';
import {featureSet} from './utils/features';
import projectMap from './utils/projectMap';

function PieceStats({tokens}) {
  if(!tokens) {
    return null;
  }

  const tokenFeatures = featureSet(tokens);

  const features = projectMap[33].features;
  
  return (
    <div>
      <h2 className="stats-header">Stats</h2>
      <div>
        {Object.keys(features).map((featureName) => {
          return (
            <div>
              <div>{featureName} ({tokenFeatures[featureName].length}/{features[featureName].length})</div>
              <div></div>
            </div>
          )
        })}
      </div>
    </div>
  );
}

export default PieceStats;
